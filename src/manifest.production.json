{
    "manifest_version": 3,
    "name": "账号保险箱",
    "description": "管理测试账号的扩展",
    "icons":{
        "16": "assets/module.png",
        "48": "assets/module.png",
        "128": "assets/module.png"
      },
    "version": "0.0.1",
    "background": {
        "service_worker": "/background.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "/content.js"
            ]
        }
    ]
}