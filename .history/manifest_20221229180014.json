{
    "name": "账号保险箱",
    "description" : "管理测试账号的扩展",
    "icons":{
      "16": "images/module.png"
    },
    "version": "0.9",
    "manifest_version": 2,
    "browser_action": {
    "default_popup": "html/index.html",
    "default_icon": "images/module.png"
    },
    "commands": {
      "_execute_browser_action": {
        "suggested_key": {
          "default": "Ctrl+Space",
          "mac": "MacCtrl+Space"
        },
        "description": "Opens index.html"
      }
    }
}