{
    "name": "账号保险箱",
    "description" : "管理测试账号的扩展",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action": {
    "default_popup": "html/index.html",
    "default_icon": "images/module.png"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Space",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Opens index.html"
    }
  }
}