{
    "manifest_version": 3,
    "name": "账号保险箱",
    "description": "管理测试账号的扩展",
    "author": "DD",
    "version": "0.0.1",
    "icons":{
        "16": "imgs/module.png",
        "48": "imgs/module.png",
        "128": "imgs/module.png"
      },
    "background": {
        "service_worker": "js/background.js"
    },
    "action": {
        "default_popup": "html/popup.html"
    },
    "permissions": [
        "storage"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "js/content.js"
            ]
        }
    ]
}